{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Alura.Estacionamento/bin/Debug/net8.0/Alura.Estacionamento.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Alura.Estacionamento",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "Docker .NET Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/Alura.Estacionamento/Alura.Estacionamento.csproj"
            }
        }
    ]
}